package cn.csbe.web.cms.user.bean;


/**
 * Description：
 * @author chzhou
 * @date    2017-12-6 上午10:10:14
 */
public class Notice{
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	private String noticeId;
	private Integer type; //0 系统通知 1物流通知  2互动通知
	private String title;
	private String spareField;//备用字段  存id/物流单号/其他
	private String introduction;
	private String leftIcon;
	private String htmlUrl;
	private Integer userId;
	private String createDate;
	private String modifyDate;
	private Integer status;//状态0无效，1有效
	private Integer infoType;//1论坛  2高僧大德
	private Integer isRead;//是否已读  0未读  1已读
	
	
	public Integer getIsRead() {
		return isRead;
	}
	public void setIsRead(Integer isRead) {
		this.isRead = isRead;
	}
	public Integer getInfoType() {
		return infoType;
	}
	public void setInfoType(Integer infoType) {
		this.infoType = infoType;
	}
	public Integer getStatus() {
		return status;
	}
	public void setStatus(Integer status) {
		this.status = status;
	}
	public String getModifyDate() {
		return modifyDate;
	}
	public void setModifyDate(String modifyDate) {
		this.modifyDate = modifyDate;
	}
	public String getCreateDate() {
		return createDate;
	}
	public void setCreateDate(String createDate) {
		this.createDate = createDate;
	}
	public Integer getUserId() {
		return userId;
	}
	public void setUserId(Integer userId) {
		this.userId = userId;
	}
	public String getNoticeId() {
		return noticeId;
	}
	public void setNoticeId(String noticeId) {
		this.noticeId = noticeId;
	}
	public Integer getType() {
		return type;
	}
	public void setType(Integer type) {
		this.type = type;
	}
	public String getTitle() {
		return title;
	}
	public void setTitle(String title) {
		this.title = title;
	}
	public String getSpareField() {
		return spareField;
	}
	public void setSpareField(String spareField) {
		this.spareField = spareField;
	}
	public String getIntroduction() {
		return introduction;
	}
	public void setIntroduction(String introduction) {
		this.introduction = introduction;
	}
	public String getLeftIcon() {
		return leftIcon;
	}
	public void setLeftIcon(String leftIcon) {
		this.leftIcon = leftIcon;
	}
	public String getHtmlUrl() {
		return htmlUrl;
	}
	public void setHtmlUrl(String htmlUrl) {
		this.htmlUrl = htmlUrl;
	}
	
}
